{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
      "rewrites": [
        { "source": "/api/:path*","destination": "http://codex-x-server.vercel.app/api/v1/:path*"},
        { "source": "/api/(.*)", "destination": "/index.js" },
        { "source": "/(.*)", "destination": "/index.js" }

    ]
}